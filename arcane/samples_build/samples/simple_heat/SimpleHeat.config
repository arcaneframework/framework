<?xml version="1.0" ?>
 <arcane-config code-name="SimpleHeat">
  <time-loops>
    <time-loop name="SimpleHeatLoop">
      <title>SimpleHeatModule</title>
      <description>Default timeloop for code SimpleHeat</description>

      <modules>
        <module name="SimpleHeat" need="required" />
        <module name="ArcanePostProcessing" need="required" />
      </modules>

      <entry-points where="init">
        <entry-point name="SimpleHeat.StartInit" />
      </entry-points>
      <entry-points where="compute-loop">
        <entry-point name="SimpleHeat.Compute" />
      </entry-points>
    </time-loop>
  </time-loops>
</arcane-config>
