<?xml version='1.0'?>
<case codeversion="1.0" codename="HelloWorld" xml:lang="en">
  <arcane>
    <title>Exemple Arcane de l'affichage de Hello World!</title>
    <timeloop>HelloWorldLoop</timeloop>
  </arcane>

  <mesh>
    <meshgenerator>
      <sod>
        <x>100</x>
        <y>5</y>
        <z>5</z>
      </sod>
    </meshgenerator>
  </mesh>
</case>
